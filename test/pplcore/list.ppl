// Test of lists

// Test int lists and concatenation
utest ([1,2]++[3])       [1,2,3];
utest ([77]++[12])       [77,12];
utest []                 [];
utest [[],[]]            [[],[]];
utest ([]++[])           [];
utest ([]++[3])          [3];
utest ([5]++[])          [5];

//Test lists of booleans
utest [true]                    [true];
utest [true && false]           [false];
utest ([true,true]++[false])      [true,true,false];

//Test lists of strings
utest ["this"]                    ["this"];
utest (["this","that"]++["and"])  ["this","that","and"];
utest (["this"++"that"]++["and"]) ["thisthat","and"]

