let observe = lam v. lam dist. weight (logpdf v dist) in

let lgss xprev data = match data with
  | [] -> xprev
  | y:ys ->
    (let x = sample (normal (xprev + 2) 1) in
    observe y (normal x 1);
    resample ();
    lgss x ys) in

let data = [24.39,25.47,29.62] in

let x0 = sample (normal 0 100) in

lgss x0 data

